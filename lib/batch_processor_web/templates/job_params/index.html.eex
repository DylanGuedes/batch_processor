<h2>Registered Jobs</h2>

<table class="ui celled table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <%= for job_params <- @job_params do %>
    <tr>
      <td><span><%= link job_params.name, to: job_params_path(@conn, :show, job_params), class: "btn btn-default btn-xs" %></span></td>
      <td><span><%= link "Edit", to: job_params_path(@conn, :edit, job_params), class: "btn btn-default btn-xs" %></span></td>
      <td>
        <%= link "Delete", to: job_params_path(@conn, :remove_job, %{"id" => job_params.id}), class: "ui button" %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Job params", to: job_params_path(@conn, :new) %></span>
